{
	"name": "dsc-best",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"server:deployment": "PORT=25606 npm run build && node build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db:deploy": "npx prisma migrate deploy",
		"db:migrate": "npx prisma migrate dev --name",
		"db:generate": "npx prisma generate && npx prisma format",
		"db:studio": "npx prisma studio --port 1234",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@smui-extra/autocomplete": "^7.0.0-beta.15",
		"@smui-extra/badge": "^7.0.0-beta.15",
		"@smui/banner": "^7.0.0-beta.15",
		"@smui/button": "^7.0.0-beta.15",
		"@smui/card": "^7.0.0-beta.15",
		"@smui/checkbox": "^7.0.0-beta.15",
		"@smui/chips": "^7.0.0-beta.15",
		"@smui/common": "^7.0.0-beta.15",
		"@smui/dialog": "^7.0.0-beta.15",
		"@smui/form-field": "^7.0.0-beta.15",
		"@smui/icon-button": "^7.0.0-beta.15",
		"@smui/layout-grid": "^7.0.0-beta.15",
		"@smui/menu": "^7.0.0-beta.15",
		"@smui/paper": "^7.0.0-beta.15",
		"@smui/segmented-button": "^7.0.0-beta.15",
		"@smui/select": "^7.0.0-beta.15",
		"@smui/switch": "^7.0.0-beta.15",
		"@smui/textfield": "^7.0.0-beta.15",
		"@smui/tooltip": "^7.0.0-beta.15",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.27.4",
		"@types/jsonwebtoken": "^9.0.5",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"smui-theme": "^7.0.0-beta.15",
		"svelte": "^4.0.5",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^0.34.0"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.6.0",
		"@smui/fab": "^7.0.0-beta.15",
		"axios": "^1.6.2",
		"cookie": "^0.6.0",
		"discord.js": "^14.14.1",
		"jsonwebtoken": "^9.0.2",
		"moment": "^2.29.4",
		"nodejs-snowflake": "^2.0.1",
		"prisma": "^5.6.0",
		"svelte-markdown": "^0.4.0",
		"svelte-material-ui": "^7.0.0-beta.15"
	}
}
